[32m+[m[32m  private Foo foo;[m
[32m+[m
[32m+[m[32m  @Before[m
[32m+[m[32m  public void setup() {[m
[32m+[m[32m    foo = new Foo();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  @Test[m
[32m+[m[32m  public void getFullNameTest() {[m
[32m+[m[32m    assertEquals("John Smith", foo.getFullName("John", "Smith"));[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
