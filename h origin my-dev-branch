[32m+[m[32mimport { NgModule } from '@angular/core';[m
[32m+[m
[32m+[m[32mimport {[m
[32m+[m[32m  EncodingService,[m
[32m+[m[32m  FirebaseService[m
[32m+[m[32m} from './services';[m
[32m+[m
[32m+[m[32m@NgModule({[m
[32m+[m[32m  providers: [[m
