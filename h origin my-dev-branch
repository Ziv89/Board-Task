[32m+[m[32m// import 'core-js/es6/string';[m
[32m+[m[32m// import 'core-js/es6/date';[m
[32m+[m[32m// import 'core-js/es6/array';[m
[32m+[m[32m// import 'core-js/es6/regexp';[m
[32m+[m[32m// import 'core-js/es6/map';[m
